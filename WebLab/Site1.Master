       function hoy()
{
   var fechaActual = new Date();
 
    dia = fechaActual.getDate();
    mes = fechaActual.getMonth() +1;
    anno = fechaActual.getYear();
    
 
    if (dia <10) dia = "0" + dia;
    if (mes <10) mes = "0" + mes;   
 
    fechaHoy = dia + "/" + mes + "/" + anno;
    
    return fechaHoy;
}
